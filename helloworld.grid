int pretty_print(int[4][4] x){
	int i;
	int j;
	for(i=0;i<4;i=i+1){
		for(j=0;j<4;j=j+1){
			print("|");
			print(x[i][j]);
			print("|");
		}
		print("\n");
	}
	return 0;
}

int gameloop(){
	int i;
	int j;
	int[4][4] y;
	for(i=0;i<4;i=i+1){
		for(j=0;j<4;j=j+1){
			y[i][j] = 999;
		}
	}
	pretty_print(y);
	return 0;
}